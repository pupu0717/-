platform :ios, '12.0'

flutter_application_path = '../..'

target 'Runner' do
  use_frameworks!

  # Flutterのポッドを追加
  pod 'Flutter', :path => File.join(flutter_application_path, '.ios', 'Flutter', 'Flutter.framework')
  pod 'FlutterPluginRegistrant', :path => File.join(flutter_application_path, '.ios', 'Flutter', 'FlutterPluginRegistrant')

  # file_picker プラグインのポッドを追加
  pod 'file_picker', :path => File.join(flutter_application_path, '.ios', 'Flutter', 'file_picker')

  # 他に必要なポッドがあればここに追加
end
