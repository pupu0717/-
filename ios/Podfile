platform :ios, '14.0'

# CocoaPods のセットアップ
# プロジェクトのルートディレクトリを指す相対パス
flutter_application_path = '../..'

# FlutterのPodfileに必要な設定をロード
#load File.join(flutter_application_path, '.ios', 'Flutter', 'pod_helper.rb')

# 必要なFlutterのポッドをインストール
install_all_flutter_pods(flutter_application_path)

target 'diving_log' do
  use_frameworks! # または use_modular_headers!

  # Flutterの依存関係をインストール
  flutter_install_all_ios_pods(flutter_application_path)

  # ここに追加のポッドを指定することができます
  # pod 'YourPodName', '~> 1.0'
end

# FlutterのPodfileの一部として、iOS 10.0以上が必要です。
# バージョンはプロジェクトの最低限サポートするiOSバージョンに合わせてください
